<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="vpicktree_inh_outpickingcarrier" model="ir.ui.view">
            <field name="name">vpicktree.inh.outpickingcarrier</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.vpicktree"/>
            <field name="arch" type="xml">
                <field name="state" position="before">
                    <field name="carrier_id" />
                    <field name="weight" />
                    <field name="weight_net" />
                </field>
            </field>
        </record>
        <record id="view_picking_internal_search_inh_outpickingcarrier" model="ir.ui.view">
            <field name="name">view.picking.internal.search.inh.outpickingcarrier</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_internal_search"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="before">
                    <field name="carrier_id" />
                </field>
                <filter string="Status" position="before">
                    <filter string="Carrier" icon="terp-personal" domain="[]" context="{'group_by':'carrier_id'}"/>
                </filter>
            </field>
        </record>
    </data>
</openerp>
